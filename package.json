{
  "name": "redux-time",
  "version": "0.0.20",
  "description": "Redux animation library that renders your state tree at a given point in time.",
  "author": "Nick Sweeting <git@nicksweeting.com>",
  "license": "MIT",
  "homepage": "https://github.com/Monadical-SAS/redux-time/",
  "bugs": {
    "url": "https://github.com/Monadical-SAS/redux-time/issues",
    "email": "redux-time-bugs@monadical.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Monadical-SAS/redux-time.git"
  },
  "directories": {
    "example": "examples"
  },
  "keywords": [
    "animation",
    "animations",
    "time",
    "time-travel",
    "warped",
    "react",
    "redux",
    "games",
    "game-engine"
  ],
  "main": "node/main.js",
  "dependencies": {
    "extend": "^3.0.1",
    "lodash.isequal": "^4.5.0",
    "warped-time": "^0.0.16"
  },
  "devDependencies": {
    "aliasify": "^2.1.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-syntax-function-bind": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-runtime": "^6.26.0",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "eslint": "^4.10.0",
    "eslint-plugin-babel": "^4.1.2",
    "flow-bin": "^0.58.0",
    "minifyify": "^7.3.5",
    "monadical-react-components": "^0.0.10",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "three": "^0.88.0",
    "uglify-js": "3.1.6",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "aliasify"
    ]
  },
  "babel": {
    "presets": [
      "react",
      "env",
      "stage-2"
    ],
    "plugins": [
      "syntax-function-bind",
      
      "transform-react-constant-elements",
      "transform-function-bind",
      "transform-runtime"
    ]
  },
  "scripts": {
    "watch": "./scripts/watch.sh",
    "build": "./scripts/build.sh",
    "test": "node node/tests.js",
    "minify": "./node_modules/.bin/uglifyjs examples/static/stress-test.js --compress --mangle --source-map --output examples/static/stress-test.min.js"
  }
}
